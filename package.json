{
  "name": "wally-prankatz-racing-school",
  "private": true,
  "description": "Wally Pankratz Racing School is the premire SoCal midget racecar school.",
  "version": "0.1.0",
  "author": "Josh Drentlaw <joshdrentlaw@gmail.com>",
  "dependencies": {
    "@sanity/block-content-to-react": "^2.0.7",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.19.17",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-netlify": "^2.1.32",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-postcss": "^2.1.20",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-source-cloudinary": "^0.1.11",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-source-sanity": "^5.0.5",
    "gatsby-transformer-sharp": "^2.3.14",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1",
    "tailwind.macro": "^0.5.10"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "tailwindcss": "^0.7.4"
  },
  "keywords": [
    "Wally",
    "Wally Prankatz"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "share": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve -H 0.0.0.0",
    "prod": "npm run build && npm run serve",
    "netlify": "netlify dev --live",
    "sanity": "cd pankratzraceschoolcom/ && sanity start",
    "sanity-deploy": "cd pankratzraceschoolcom/ && sanity deploy --no-minify",
    "sanity-graphql": "cd pankratzraceschoolcom/ && sanity graphql deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JoshDrentlaw/wally-racing"
  },
  "bugs": {
    "url": "https://github.com/JoshDrentlaw/wally-racing/issues"
  }
}
